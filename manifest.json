{
	"manifest_version": 2,
	"background": { "scripts" : [ "js/jquery.js", "js/waper.js", "js/background.js" ] },
	"browser_action": {
		"default_icon": "icon.png"
	},
	"description": "This extension fully integrates Waper into Chrome browser. Better notifications, better messaging, better browsing.",
	"icons": {
		"128": "icon_128.png"
	},
	"name": "Waper",
	"permissions": [ "tabs", "notifications", "http://waper.ru/" ],
	"web_accessible_resources": [ "icon_128.png" ],
	"version": "0.1"
}